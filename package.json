{
  "name": "mango-media",
  "version": "1.0.0",
  "description": "Simple Social Media",
  "main": "index.js",
  "engines": {
    "node": "15.5.0",
    "npm": "7.3.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint -c ./.eslintrc.js ./src",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziroock/mango-media.git"
  },
  "author": "Zaprin Ignatiev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ziroock/mango-media/issues"
  },
  "homepage": "https://github.com/ziroock/mango-media#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.2.6",
    "aws-sdk": "^2.802.0",
    "babel-eslint": "^10.1.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.2.0",
    "connect-flash": "^0.1.1",
    "cookie-session": "^1.4.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "\"prettier\": \"^1.6.1\",": "^1.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http": "0.0.1-security",
    "image-size": "^0.9.3",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.8",
    "mquery": ">=3.2.3",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path-parser": "^6.1.0",
    "prettier": "^1.6.1",
    "sendgrid": "^5.2.3",
    "url": "^0.11.0"
  }
}
